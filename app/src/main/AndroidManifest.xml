<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.valensas.kyc_android">

    <uses-permission android:name="android.permission.CAMERA" />

    <meta-data
        android:name="com.google.firebase.ml.vision.DEPENDENCIES"
        android:value="ocr,face,barcode" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".facedetection.FaceDetectionActivity" />
        <activity android:name=".qrreader.QRReaderActivity" />
        <activity
            android:name=".identityviacamera.IdentityCameraActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".identitysigniture.IdentitySignitureActivity"
            android:configChanges="orientation"
            android:screenOrientation="landscape" />
        <activity android:name=".identityresult.IdentityResultActivity"></activity>
    </application>

</manifest>